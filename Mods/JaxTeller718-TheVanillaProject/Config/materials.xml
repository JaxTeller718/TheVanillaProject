<JaxTeller718>
	<append xpath="/materials">
		<material id="MresourceHubcap">
			<property name="damage_category" value="metal"/>
			<property name="surface_category" value="metal"/>
			<property name="forge_category" value="iron"/>
			<property name="Experience" value="8"/>
		</material>
		
		<material id="MresourceStick">
			<property name="damage_category" value="wood"/>
			<property name="surface_category" value="wood"/>
			<property name="forge_category" value="stick"/>
			<property name="Experience" value="1.5"/>
		</material>
		
		<material id="MresourceLeatherStrips">
			<property name="damage_category" value="cloth"/>
			<property name="surface_category" value="cloth"/>
			<property name="forge_category" value="leatherstrip"/>
			<property name="Experience" value="4"/>
		</material>
		
	</append>
	
	<set xpath="/materials/material[@id='MtreeWoodSmall']/property[@name='MaxDamage']/@value">150</set>
	<set xpath="/materials/material[@id='MtreeWoodMedium']/property[@name='MaxDamage']/@value">250</set>
	<set xpath="/materials/material[@id='MtreeWoodLarge']/property[@name='MaxDamage']/@value">300</set>
	<set xpath="/materials/material[@id='MtreeWoodFull']/property[@name='MaxDamage']/@value">500</set>
	
</JaxTeller718>