<?xml version="1.0" encoding="UTF-8" ?>



<configs>

	<append xpath="/items">	


	<!-- LEDGER MALE-->

<item name="armorEnforcerOutfit_M_ledger" name_key="armorEnforcerOutfit_M_ledger"> 
	<property name="Extends" value="armorLightMaster" param1="CustomIcon"/>
	<property name="DescriptionKey" value="armorEnforcerOutfitDesc"/>
	<property name="DisplayType" value="armorEnforcerOutfit"/>
	<property name="ArmorGroup" value="groupEnforcer"/>
	<property name="Tags" value="upperbody,chest,armor,armorChest,canHaveCosmetic,lightArmor,lightArmorDeg,armorEnforcerCritResist"/>
	<property name="EquipSlot" value="Chest"/>
	<property name="SoundPickup" value="outfit_cloth_grab"/>
	<property name="SoundPlace" value="outfit_cloth_place"/>
	<property name="CustomIcon" value="M_enforcer_male_outfit_ledger"/> 

	<effect_group name="EnforcerOutfit">
		<passive_effect name="ModSlots" operation="base_set" value="1,1,2,2,3,4" tier="1,2,3,4,5,6"/>
		<passive_effect name="ModPowerBonus" operation="base_add" value="300" tags="EconomicValue"/>
		<passive_effect name="PhysicalDamageResist" operation="base_add" value="8,12.3" tier="1,6"/>
		<passive_effect name="PhysicalDamageResist" operation="base_add" value="-.2,.2"/>
		<passive_effect name="ElementalDamageResist" operation="base_add" value="8,12.3" tier="1,6" tags="heat,electrical"/>
		<passive_effect name="ElementalDamageResist" operation="base_add" value="-.2,.2"/>
		<passive_effect name="BuffResistance" operation="base_add" value="0.041" tags="buffFatiguedTrigger,buffArmSprainedCHTrigger,buffLegSprainedCHTrigger,buffAbrasionCatch,buffLaceration,buffInfectionCatch,buffInjuryStunned01CHTrigger,buffInjuryBleedingTwo,buffInjuryBleedingBarbedWire"/>
		<passive_effect name="DegradationMax" operation="base_set" value="250,500" tier="1,6"/>
		<passive_effect name="DegradationPerUse" operation="base_set" value="1" tags="lightArmorDeg"/>
		<passive_effect name="HypothermalResist" operation="base_add" value="20"/>
		<passive_effect name="HyperthermalResist" operation="base_add" value="20"/>
		
		<display_value name="dCritResist" value=".05,.1,.15,.2,.25,.4" tier="1,2,3,4,5,6"/>
		<triggered_effect trigger="onSelfEquipUpdate" action="ModifyCVar" cvar="$enforcerBuffResistance" operation="set" value=".05,.1,.15,.2,.25,.4"/>
	</effect_group>

	<!-- Enforcer Vol 02 -->
	<effect_group>
		<passive_effect name="GeneralDamageResist" operation="base_add" value="0.05">
			<requirement name="ProgressionLevel" progression_name="perkEnforcerApparel" operation="Equals" value="1"/>
		</passive_effect>
	</effect_group>

	<effect_group>
		<triggered_effect trigger="onSelfEquipStop" action="RemoveCVar" cvar="$enforcerBuffResistance"/>
	</effect_group>


	<effect_group>
				<triggered_effect trigger="onSelfEquipChanged" action="SetTransformActive" active="true" transform_path="bodyTP">
					<requirement name="!IsFPV"/>
				</triggered_effect>
				<triggered_effect trigger="onSelfEquipChanged" action="SetTransformActive" active="false" transform_path="bodyTP">
					<requirement name="IsFPV"/>
				</triggered_effect>
			</effect_group>
			
	<property class="SDCS">
		<property name="Prefab" value="#@modfolder:Resources/M_enforcer_male_variants.unity3d?M_enforcer_ledger"/>
		<property name="TransformName" value="body"/>
		<property name="Excludes" value="body"/>
	</property>
</item>

